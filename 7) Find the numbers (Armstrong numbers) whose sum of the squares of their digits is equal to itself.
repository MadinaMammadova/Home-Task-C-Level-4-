#include <iostream>
#include <cmath>
using namespace std;
int main() {
    int i=1,s=0,m=0,n,a,k,b;
    cout<<"Insert Number = ";
    cin>>n;
    a=n;
    b=n;
while(n>0){
    m=m+1;
    n=n/10;
}
while(b>0){
    k=b%10;
    s=s+pow(k,m);
    b=b/10;
}
if(s==a){
    cout<<"Number is Armstrong";
}
else 
    cout<<"Number is not Armstrong";
    return 0;
}

#include <iostream>
using namespace std;
int main() {
    int a,b,c;
    cout<<"First Side= ";
    cin>>a;
    cout<<"Second Side = ";
    cin>>b;
    cout<<"Third Side = ";
    cin>>c;
if(a+b>=c && a+c>b && b+c>>a){
    cout<<"Perimeter = "<<a+b+c;
}
else
   cout<<"Can't make a Triangle";
  return 0;
}
